// Online C compiler to run C program online
// C program for the above approach
#include <stdio.h>
//To find square root of a number
int main(){
    double number,sqroot;
    printf("Enter a number:");
    scanf("%lf",&number);
    sqroot=sqrt(number);
    printf("The square root of %.2lf is: %.2lf",number,sqroot);
    return 0;
}
